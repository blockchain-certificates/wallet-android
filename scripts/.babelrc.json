{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "chrome > 66",
        "modules": false,
        "loose": false,
        "debug": true,
        "useBuiltIns": "usage",
        "corejs": "3.4"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-optional-chaining",
    "@babel/plugin-transform-nullish-coalescing-operator",
    "@babel/plugin-syntax-class-properties"
    "babel-plugin-transform-globalthis"
//     [
//       "babel-plugin-polyfill-corejs3",
//       {
//         "method": "entry-global",
//         "version": "3.4",
//         "debug": true
//       }
//     ]
  ]
}
